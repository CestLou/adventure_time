<%= semantic_form_for @report_card do |f| %>
  <%= f.inputs do %>
    <%= f.input :test_id %>
    <%= f.input :user_id %>
    <%= f.input :grade %>
    <%= f.input :name %>
    <%= f.input :finished %>
    <%= f.input :current_question %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
